{
    "version": 2,
    "builds": [
        {
          "src": "flask_app.py",
          "use": "@vercel/python",
          "config": { "buildCommand": "bash   build.sh " }
         
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "flask_app.py"
        }
      ],
      "functions": {
        "api/**/*.py": {
          "runtime": "python3.12",
          "zipped": true,
          "maxSize": "250mb"
        }
      }
  }